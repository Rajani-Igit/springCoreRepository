<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="radio" class="com.awareinjection.beans.Radio">
    <property name="frequency" value="101.3"></property>
    </bean>
    <bean id="lowFrequency" class="com.awareinjection.beans.LowFrequencyReciver"></bean>
    <bean id="highFrequency" class="com.awareinjection.beans.HighFrequencyReciver"></bean>
</beans>
