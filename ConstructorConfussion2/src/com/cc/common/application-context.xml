<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="machine" class="com.cc.beans.Machine">
    <constructor-arg value="10" name="id"></constructor-arg>
    <constructor-arg value="MotorPump" name="name"></constructor-arg>
    <constructor-arg value="123456" name="productNo"></constructor-arg>
    </bean>

</beans>
