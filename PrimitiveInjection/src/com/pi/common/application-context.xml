<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="laptop" class="com.pi.beans.Laptop">
    <constructor-arg value="Sony" type="java.lang.String"></constructor-arg>
    <constructor-arg value="45000" type="long"></constructor-arg>
    <property name="serialNo" value="123456"></property>
    <property name="batteryPersent" value="86.6"></property>
    <property name="producyKey" value="4456.4456"></property>
    </bean>
</beans>
